pipeline {
    agent any
    
    stages {
        stage("Checkout") {
            steps {
                git branch: "master", url: "https://github.com/masalinas/poc-simple-java.git"
            }
        }

        stage('Deploying into k8s'){
            steps{
                sh 'kubectl apply -f deployment.yml'
            }
        }              
    }
}
